<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600" prefWidth="900" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dadosmeteorologicos.controller.ValorMedioController">
    <children>
        <Label layoutX="104.0" layoutY="90.0" text="Cidade:">
            <font>
                <Font name="Yu Gothic Regular" size="24.0" />
            </font>
        </Label>
        <Label layoutX="137.0" layoutY="183.0">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
      <MenuButton fx:id="menuButton" layoutX="198.0" layoutY="88.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="574.0" text="Selecione a cidade">
      </MenuButton>
      <DatePicker fx:id="dataInicial" layoutX="258.0" layoutY="205.0" promptText="DATA FINAL" />
      <DatePicker fx:id="dataFinal" layoutX="456.0" layoutY="205.0" promptText="DATA FINAL" />
      <Button fx:id="executar" layoutX="363.0" layoutY="401.0" mnemonicParsing="false" onAction="#onExecutarClicked" prefHeight="45.0" prefWidth="126.0" text="Executar" />
    </children>
    <cursor>
        <Cursor fx:constant="DEFAULT" />
    </cursor>
</AnchorPane>
